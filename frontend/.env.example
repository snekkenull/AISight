# API Configuration
VITE_API_URL=http://localhost:3000
VITE_WS_URL=ws://localhost:3000

# Map Configuration
VITE_MAP_DEFAULT_LAT=37.7749
VITE_MAP_DEFAULT_LON=-122.4194
VITE_MAP_DEFAULT_ZOOM=10

# Feature Flags
VITE_ENABLE_VESSEL_TRACKS=true
VITE_ENABLE_CLUSTERING=true

# Environment
VITE_ENV=development

# AI/LLM Configuration
VITE_LLM_API_BASE_URL=https://api.groq.com/openai/v1
VITE_LLM_MODEL=moonshotai/kimi-k2-instruct-0905
VITE_LLM_API_KEY=

# Position Data Age Thresholds (in hours)
# These thresholds determine how position data is classified:
# - ACTIVE: < VITE_POSITION_STALE_THRESHOLD_HOURS
# - STALE: VITE_POSITION_STALE_THRESHOLD_HOURS to VITE_POSITION_VERY_STALE_THRESHOLD_HOURS
# - VERY_STALE: VITE_POSITION_VERY_STALE_THRESHOLD_HOURS to VITE_POSITION_NO_DATA_THRESHOLD_HOURS
# - NO_DATA: > VITE_POSITION_NO_DATA_THRESHOLD_HOURS or missing
VITE_POSITION_STALE_THRESHOLD_HOURS=1
VITE_POSITION_VERY_STALE_THRESHOLD_HOURS=24
VITE_POSITION_NO_DATA_THRESHOLD_HOURS=168

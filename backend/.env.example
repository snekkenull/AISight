# AISStream API Configuration
AISSTREAM_API_KEY=your_aisstream_api_key_here

# Database Configuration
DATABASE_URL=postgresql://ais_user:ais_password@localhost:5432/ais_db
DB_HOST=localhost
DB_PORT=5432
DB_NAME=ais_db
DB_USER=ais_user
DB_PASSWORD=ais_password
DB_POOL_MIN=2
DB_POOL_MAX=10

# Redis Configuration
REDIS_URL=redis://localhost:6379
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# Server Configuration
NODE_ENV=development
PORT=3000
HOST=0.0.0.0

# CORS Configuration
CORS_ORIGIN=http://localhost:5173

# Logging Configuration
LOG_LEVEL=info

# Cache TTL (in seconds)
CACHE_VESSEL_POSITION_TTL=60
CACHE_VESSEL_METADATA_TTL=3600

# Data Pipeline Configuration
BATCH_SIZE=100
BATCH_INTERVAL_MS=1000

# WebSocket Configuration
WS_PING_INTERVAL=30000
WS_PING_TIMEOUT=5000

# Regional Scheduler Configuration
# Duration per region in milliseconds (default: 4 hours = 14400000)
REGION_DURATION_MS=14400000
# Enable automatic region rotation (default: true)
REGION_AUTO_ROTATE=true
